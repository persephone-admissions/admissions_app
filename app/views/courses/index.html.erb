<div class="course-wrapper"> 
  <div class="course-container">
    <h1>Courses</h1>
    <!-- <ul> -->
    <div class="courses-list">
      <ul>
       <% @courses.each do |course| %>
            <li> <%= link_to course.name, course_path(course) %> Starts: <%= course.start_on.strftime("%m/%d/%Y") if course.start_on %> Ends: <%= course.end_on.strftime("%m/%d/%Y") if course.end_on %></li>
       <% end %>
      </ul>
    </div>
  <!--   </ul> -->
    <% if @current_user %>
      <% if @current_user.type == "Instructor" || @current_user.type == "Officer" %>
        <div class="new-course-link">
          <%= link_to 'New Course', new_course_path %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>



















